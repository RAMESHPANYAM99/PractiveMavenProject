
pipeline{
agent any
stages{
stage("Build")
{
steps
{
echo "Building the Project"
sh "mvn clean compile"
}
}
stage("Testing the code"){
steps{
echo "Testing the code"
sh "mvn test"
}
}
stage("Compiling the code"){
steps{
echo "Compiling the code"
sh "mvn compile"
}
}
stage("Deploying the code"){
steps{
echo "Deploying the code"
}
}
}
}
